<t tx="swot.20240224200843.5">@language python
def renumber_child_headlines(p, nb: str) -&gt; None:
    """ nb means numbering """
    num_temp = 1

    for node in p.children():
        # skip @ignore-node @ignore-tree no-head
        if node.h.startswith(("@ignore", "no-head")):
            continue

        new_nb = f"{nb}{num_temp}"
        if not nb:
            # nb may be empty only at first level
            new_nb += "."

        node.h = f"{new_nb} {node.h}"
        num_temp += 1
