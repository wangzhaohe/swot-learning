spring:
  cloud:
    gateway:
      routes:
        - id: order-route
          uri: lb://app-order
          # -- 长写法 --
          predicates:
            - name: Path
              args:
                patterns: /api/order/**
          # -- 长写法结束 --
          order: 1

          # --New Added Start--
          filters:  # /api/a/b -&gt; /a/b  &lt;1&gt;
            - RewritePath=/api/(?&lt;segment&gt;.*), /$\{segment}
            - AddResponseHeader=X-Response-Red, Blue
          # --New Added End--

        - id: item-route
          uri: lb://app-item
          predicates:
            - Path=/api/item/**
          order: 2
          filters:  # 同上
            - RewritePath=/api/(?&lt;segment&gt;.*), /$\{segment}
        @others
