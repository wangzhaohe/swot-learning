@Test
public void generateJwt() {

    // 1.  jwt 
    Map&lt;String, Object&gt; claims = new HashMap&lt;&gt;();
    claims.put("id", 1);
    claims.put("username", "limuwan");

    // 2. 256 bit 32 
    //    32256256 bit
    SecretKey key = Keys.hmacShaKeyFor("12345678901234567890123456789012".getBytes());

    // 3.  JWT0.12.x 
    String jwt = Jwts.builder()
            .signWith(key, Jwts.SIG.HS256)                    // 
            .claims(claims)                                   // 
           // .expiration(new Date(System.currentTimeMillis() + 12 * 3600 * 1000)) // 12h 
           // .expiration(new Date(System.currentTimeMillis() - 1000)) //  1s 
            .expiration(new Date(System.currentTimeMillis() + 60 * 1000)) // 1min 
            .compact();  // 
    System.out.println(jwt);
}
