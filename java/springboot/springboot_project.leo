public static String generateJwt(Map&lt;String, Object&gt; claims) {
    String jwt = Jwts.builder()
            .signWith(key, Jwts.SIG.HS256)                    // 
            .claims(claims)                                   // 
            .expiration(new Date(System.currentTimeMillis() + expire))
            .compact();  // 
    return jwt;
}
