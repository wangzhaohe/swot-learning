    String jwt = Jwts.builder()
            .signWith(key, Jwts.SIG.HS256)                    // 先密钥，再算法
            .claims(claims)                                   // 设置自定义数据
            .expiration(new Date(System.currentTimeMillis() + expire))
            .compact();  // 生成令牌
    return jwt;
}
